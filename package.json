{
  "name": "0data",
  "displayName": "0DataCleaner",
  "description": "An Ontology based data cleaning extension which will do the basic data cleaning for you.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:0data.helloWorld",
    "onCommand:0data.handleMissing",
    "onCommand:0data.removeDuplicate",
    "onCommand:0data.fixStructure",
    "onCommand:0data.cleanEmptyRows"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "0data.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "0data.loadExcel",
        "title": "Load Excel"
      },
      {
        "command": "0data.handleMissing",
        "title": "0Data: Clean Empty Rows"
      },
      {
        "command": "0data.removeDuplicate",
        "title": "0Data: Remove Duplicate Entry"
      },
      {
        "command": "0data.fixStructure",
        "title": "0Data: Fix Structure"
      },
      {
        "command": "0data.fillWithZero",
        "title": "0Data: Fill Missing Values With Zero"
      },
      {
        "command": "0data.cleanEmptyRows",
        "title": "0Data: Fill Missing Values With Average"
      }
      
      
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .xls || resourceExtname == .xlsx",
          "command": "0data.handleMissing",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .xls || resourceExtname == .xlsx",
          "command": "0data.removeDuplicate",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .xls || resourceExtname == .xlsx",
          "command": "0data.fixStructure",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .xls || resourceExtname == .xlsx",
          "command": "0data.cleanEmptyRows",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "exceljs": "^4.3.0",
    "fs": "^0.0.1-security",
    "json-as-xlsx": "^2.3.0",
    "node-xlsx": "^0.17.2",
    "read-excel-file": "^5.2.25"
  }
}
